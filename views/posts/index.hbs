

<h1>Timeline</h1>
<ul class="posts">
  {{#each posts}}
  <li class="post">{{this.message}}<br>
    {{this.date}}<br>
    {{this.userName}}
    <img src= {{this.profPic}} width="40" height="50">
  {{/each}}
</ul>



